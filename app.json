{
  "expo": {
    "notification": {
      "icon": "./assets/images/notification_icon.png",
      "color": "#1142BE"
    },
    "name": "emergency-alert-system",
    "slug": "clinic-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yabashiaka.clinicapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "VIBRATE"
      ],
      "package": "com.yabashiaka.clinicapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "assetBundlePatterns": [
      "**/*",
      "node_modules/react-native-vector-icons/Fonts/*"
    ],
    "plugins": [
      "expo-router",
      "expo-background-fetch",
      "expo-task-manager",
      "expo-av",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification_icon.png",
          "color": "#1142BE",
          "sounds": [
            "./assets/sound/emergency_alert.wav"
          ]
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c90572c1-39a6-414b-9547-eac1231e32e6"
      }
    },
    "owner": "yabashiaka",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c90572c1-39a6-414b-9547-eac1231e32e6"
    }
  }
}
