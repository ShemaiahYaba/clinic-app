{
  "expo": {
    "name": "MTU-RapidCare",
    "slug": "mtu-rapidcare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mturapidcare",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yabashiaka.mturapidcare",
      "googleServicesFile": "./google-services.json",
      "buildType": "apk"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      [
        "expo-notifications",
        {
          "sounds": ["./assets/sounds/emergency_alert.wav"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "75a97b04-b497-44fb-8ed7-c37df4097415"
      }
    },
    "owner": "yabashiaka",
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/75a97b04-b497-44fb-8ed7-c37df4097415"
    }
  }
}
